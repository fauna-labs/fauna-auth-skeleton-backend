CreateFunction({
  name: "reset_password",
  body: Query(Lambda(["password"], Let([{
    resetRequest: Get(CurrentIdentity())
  }, {
    accountRef: Select(["data", "account"], Var("resetRequest"))
  }, {
    account: Update(Var("accountRef"), {
      credentials: {
        password: Var("password")
      }
    })
  }], Do(Delete(CurrentToken()), Var("account"))))),
  role: "server"
})