CreateFunction({
  "name": "get_config_var",
  "body": Query(Lambda(["pathObj"], Let([{
    "configPath": Select(["path"], Var("pathObj"))
  }], Select(Var("configPath"), Let([{
    "configSet": Documents(Collection("config"))
  }], If(Exists(Var("configSet")), Select(["data"], Get(Var("configSet"))), {
    "call_limits": {
      "max_failed_logins": 5
    }
  })), Select(Var("configPath"), {
    "call_limits": {
      "max_failed_logins": 5
    }
  }))))),
  "role": "server"
})