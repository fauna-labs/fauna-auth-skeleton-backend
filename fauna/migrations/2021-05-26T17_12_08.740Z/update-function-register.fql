Update(Function("register"), {
  "body": Query(Lambda(["email", "password"], Do(Call("rate_limit", ["register", "public", 100, 3600000]), Call("validate_email", Var("email")), Call("validate_password", Var("password")), Create(Collection("accounts"), {
    "credentials": {
      "password": Var("password")
    },
    "data": {
      "email": Var("email")
    }
  })))),
  "role": "server",
  "data": null
})